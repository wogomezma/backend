<div>
    <h1>Products Real Time</h1>
</div>

<section>
<div>
    <h1>Eliminar Producto</h1>
    <p>Ingrese el ID del producto que desea eliminar y presione enter</p>
    <input id="textbox">
    <h2>Nueva Actualizacion de productos despues de eliminar</h2>
    <p id="log"></p>
</div>
<br></br>
<hr>

</section>
<section>
<h1>Crear Producto</h1>
<form onsubmit="return addMessageadd(this)">
  <label for="campo1">Title:</label>
  <input type="text" id="addtitle" name="addtitle"><br>

  <label for="campo2">Description:</label>
  <input type="text" id="adddescription" name="adddescription"><br>

  <label for="campo3">Price:</label>
  <input type="text" id="addprice" name="addprice"><br>

  <label for="campo4">Thumbnail:</label>
  <input type="text" id="addthumbnail" name="addthumbnail"><br>

  <label for="campo5">Stock:</label>
  <input type="text" id="addstock" name="addstock"><br>

  <label for="campo6">Code:</label>
  <input type="text" id="addcode" name="addcode"><br>

  <input type="submit" value="Crear">
</form>


</section>
<section>
    <h1>Historico de Productos</h1>
        {{{body}}}
</section>


<script src="/socket.io/socket.io.js"></script>
